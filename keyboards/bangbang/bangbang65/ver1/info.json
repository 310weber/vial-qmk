{
  "keyboard_name": "BangBang65 v1",
  "height": 5,
  "width": 13,
  "usb": {
    "pid": "0x6500",
    "device_version": "1.0.0"
  },

  "processor": "STM32F401",
  "board": "GENERIC_STM32_F401XC",
  "bootloader": "stm32-dfu",
  
  "features":{
    "bootmagic": true,
    "command": false,
    "console": false,
    "encoder": true,
    "extrakey": true,
    "mousekey": false,
    "nkro": false,
    "rgblight": true,
    "sleep_led": false
  },

  "build": {
    "firmware_format": "bin",
    "lto": true
  },

  "encoder":{
    "rotary":[
      {"pin_a": "C10", "pin_b": "C11", "resolution": 2}
    ]
  },

  "indicators": {
    "caps_lock": "A1",
    "num_lock":  "A2"
  },
 
   "layout_aliases":{
    "KEYMAP":"LAYOUT_base",
    "Base_Layout":"LAYOUT_base"
  },

  "layouts": {
    "LAYOUT_base": {
      "layout": [
        { "label": "~", "matrix": [0, 0], "x": 0, "y": 0 },
        { "label": "!", "matrix": [0, 1], "x": 1, "y": 0 },
        { "label": "@", "matrix": [0, 2], "x": 2, "y": 0 },
        { "label": "#", "matrix": [0, 3], "x": 3, "y": 0 },
        { "label": "$", "matrix": [0, 4], "x": 4, "y": 0 },
        { "label": "%", "matrix": [0, 5], "x": 5, "y": 0 },
        { "label": "^", "matrix": [0, 6], "x": 6, "y": 0 },
        { "label": "&", "matrix": [5, 0], "x": 8, "y": 0 },
        { "label": "*", "matrix": [5, 1], "x": 9, "y": 0 },
        { "label": "(", "matrix": [5, 2], "x": 10, "y": 0 },
        { "label": ")", "matrix": [5, 3], "x": 11, "y": 0 },
        { "label": "_", "matrix": [5, 4], "x": 12, "y": 0 },
        { "label": "+", "matrix": [5, 5], "x": 13, "y": 0 },
        { "label": "Backspace", "matrix": [5, 6], "x": 14.25, "y": 0 },
        { "label": "Tab", "matrix": [1, 0], "x": 0.5, "y": 1 },
        { "label": "Q", "matrix": [1, 1], "x": 1.5, "y": 1 },
        { "label": "W", "matrix": [1, 2], "x": 2.5, "y": 1 },
        { "label": "E", "matrix": [1, 3], "x": 3.5, "y": 1 },
        { "label": "R", "matrix": [1, 4], "x": 4.5, "y": 1 },
        { "label": "T", "matrix": [1, 5], "x": 5.5, "y": 1 },
        { "label": "Y", "matrix": [6, 0], "x": 7.5, "y": 1 },
        { "label": "U", "matrix": [6, 1], "x": 8.5, "y": 1 },
        { "label": "I", "matrix": [6, 2], "x": 9.5, "y": 1 },
        { "label": "O", "matrix": [6, 3], "x": 10.5, "y": 1 },
        { "label": "P", "matrix": [6, 4], "x": 11.5, "y": 1 },
        { "label": "{", "matrix": [6, 5], "x": 12.5, "y": 1 },
        { "label": "}", "matrix": [6, 6], "x": 13.5, "y": 1 },
        { "label": "|", "matrix": [6, 7], "x": 14.5, "y": 1 },
        { "label": "Caps Lock", "matrix": [2, 0], "x": 0.75, "y": 2 },
        { "label": "A", "matrix": [2, 1], "x": 1.75, "y": 2 },
        { "label": "S", "matrix": [2, 2], "x": 2.75, "y": 2 },
        { "label": "D", "matrix": [2, 3], "x": 3.75, "y": 2 },
        { "label": "F", "matrix": [2, 4], "x": 4.75, "y": 2 },
        { "label": "G", "matrix": [2, 5], "x": 5.75, "y": 2 },
        { "label": "H", "matrix": [7, 0], "x": 7.75, "y": 2 },
        { "label": "J", "matrix": [7, 1], "x": 8.75, "y": 2 },
        { "label": "K", "matrix": [7, 2], "x": 9.75, "y": 2 },
        { "label": "L", "matrix": [7, 3], "x": 10.75, "y": 2 },
        { "label": ":", "matrix": [7, 4], "x": 11.75, "y": 2 },
        { "label": "\"", "matrix": [7, 5], "x": 12.75, "y": 2 },
        { "label": "Enter", "matrix": [7, 6], "x": 13.75, "y": 2 },
        { "label": "Shift", "matrix": [3, 0], "x": 1, "y": 3 },
        { "label": "Z", "matrix": [3, 1], "x": 2.25, "y": 3 },
        { "label": "X", "matrix": [3, 2], "x": 3.25, "y": 3 },
        { "label": "C", "matrix": [3, 3], "x": 4.25, "y": 3 },
        { "label": "V", "matrix": [3, 4], "x": 5.25, "y": 3 },
        { "label": "B", "matrix": [3, 5], "x": 6.25, "y": 3 },
        { "label": "N", "matrix": [8, 0], "x": 8.25, "y": 3 },
        { "label": "M", "matrix": [8, 1], "x": 9.25, "y": 3 },
        { "label": "<", "matrix": [8, 2], "x": 10.25, "y": 3 },
        { "label": ">", "matrix": [8, 3], "x": 11.25, "y": 3 },
        { "label": "?", "matrix": [8, 4], "x": 12.25, "y": 3 },
        { "label": "Shift", "matrix": [8, 5], "x": 13.25, "y": 3 },
        { "label": "Up", "matrix": [8, 6], "x": 14.5, "y": 3.25 },
        { "label": "Ctrl", "matrix": [4, 0], "x": 0.75, "y": 4 },
        { "label": "Win", "matrix": [4, 1], "x": 1.75, "y": 4 },
        { "label": "Alt", "matrix": [4, 2], "x": 2.75, "y": 4 },
        { "label": "Raise", "matrix": [4, 3], "x": 4, "y": 4 },
        { "label": "Delete", "matrix": [4, 4], "x": 5.25, "y": 4 },
        { "label": "Space", "matrix": [4, 5], "x": 6.25, "y": 4 },
        { "label": "Enter", "matrix": [9, 0], "x": 7.5, "y": 4 },
        { "label": "Backspace", "matrix": [9, 1], "x": 8.5, "y": 4 },
        { "label": "Lower", "matrix": [9, 2], "x": 9.75, "y": 4 },
        { "label": "Alt", "matrix": [9, 3], "x": 11, "y": 4 },
        { "label": "Ctrl", "matrix": [9, 4], "x": 12, "y": 4 },
        { "label": "Left", "matrix": [9, 5], "x": 13.5, "y": 4.25 },
        { "label": "Down", "matrix": [9, 6], "x": 14.5, "y": 4.25 },
        { "label": "Right", "matrix": [9, 7], "x": 15.5, "y": 4.25 }
      ]
    }
  },

  "diode_direction": "COL2ROW",
  "matrix_pins": {
      "cols": ["B10", "B1",  "B0", "C5", "C4", "A7",  "A6",  "B9"],
      "rows": ["B13", "B12", "A5", "A4", "A3", "A15", "C12", "C0", "C1", "C2"]
  },
 
  "rgblight": {
    "led_count": 13,
    "animations": {
      "alternating": false,
      "breathing": true,
      "christmas": false,
      "knight": false,
      "rainbow_mood": true,
      "rainbow_swirl": true,
      "rgb_test": false, 
      "snake": true,
      "static_gradient": true,
      "twinkle": true
    },
    "sleep": true,
    "hue_steps": 4,
    "saturation_steps": 17,
    "brightness_steps": 9
  },

  "ws2812": {
    "pin": "B14",
    "driver": "pwm"
  }

}
