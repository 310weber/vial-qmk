{
  "keyboard_name": "BangBang60 v2",
  "height": 5,
  "width": 13,
  "usb": {
    "pid": "0x6000",
    "device_version": "2.0.0"
  },
 
  "processor": "atmega32u4",
  "bootloader": "caterina",

  "features":{
    "bootmagic": false,
    "command": false,
    "console": false,
    "encoder": true,
    "extrakey": true,
    "mousekey": false,
    "nkro": false,
    "rgblight": true,
    "sleep_led": false
  },

  "build": {
    "firmware_format": "hex",
    "lto": true
  },

  "encoder":{
    "rotary":[
      {"pin_a": "B0", "pin_b": "E6", "resolution": 2}
    ]
  },

  "indicators": {
    "caps_lock": "B1",
    "num_lock":  "C6"
  },
 
  "layout_aliases":{
    "KEYMAP":"LAYOUT_base",
    "Base_Layout":"LAYOUT_base"
  },

  "layouts": {
    "LAYOUT_base": {
      "layout": [
        { "label": "~", "matrix": [0, 0], "x": 0, "y": 0 },
        { "label": "!", "matrix": [0, 1], "x": 1, "y": 0 },
        { "label": "@", "matrix": [0, 2], "x": 2, "y": 0 },
        { "label": "#", "matrix": [0, 3], "x": 3, "y": 0 },
        { "label": "$", "matrix": [0, 4], "x": 4, "y": 0 },
        { "label": "%", "matrix": [0, 5], "x": 5, "y": 0 },
        { "label": "^", "matrix": [0, 6], "x": 6, "y": 0 },
        { "label": "&", "matrix": [0, 7], "x": 8, "y": 0 },
        { "label": "*", "matrix": [0, 8], "x": 9, "y": 0 },
        { "label": "(", "matrix": [0, 9], "x": 10, "y": 0 },
        { "label": ")", "matrix": [0, 10], "x": 11, "y": 0 },
        { "label": "_", "matrix": [0, 11], "x": 12, "y": 0 },
        { "label": "+", "matrix": [0, 12], "x": 13, "y": 0 },
        { "label": "Backspace", "matrix": [1, 12], "x": 14.25, "y": 0 },
        { "label": "Tab", "matrix": [1, 0], "x": 0.5, "y": 1 },
        { "label": "Q", "matrix": [1, 1], "x": 1.5, "y": 1 },
        { "label": "W", "matrix": [1, 2], "x": 2.5, "y": 1 },
        { "label": "E", "matrix": [1, 3], "x": 3.5, "y": 1 },
        { "label": "R", "matrix": [1, 4], "x": 4.5, "y": 1 },
        { "label": "T", "matrix": [1, 5], "x": 5.5, "y": 1 },
        { "label": "Y", "matrix": [1, 6], "x": 7.5, "y": 1 },
        { "label": "U", "matrix": [1, 7], "x": 8.5, "y": 1 },
        { "label": "I", "matrix": [1, 8], "x": 9.5, "y": 1 },
        { "label": "O", "matrix": [1, 9], "x": 10.5, "y": 1 },
        { "label": "P", "matrix": [1, 10], "x": 11.5, "y": 1 },
        { "label": "{", "matrix": [1, 11], "x": 12.5, "y": 1 },
        { "label": "}", "matrix": [3, 12], "x": 13.5, "y": 1 },
        { "label": "|", "matrix": [2, 12], "x": 14.5, "y": 1 },
        { "label": "Caps Lock", "matrix": [2, 0], "x": 0.75, "y": 2 },
        { "label": "A", "matrix": [2, 1], "x": 1.75, "y": 2 },
        { "label": "S", "matrix": [2, 2], "x": 2.75, "y": 2 },
        { "label": "D", "matrix": [2, 3], "x": 3.75, "y": 2 },
        { "label": "F", "matrix": [2, 4], "x": 4.75, "y": 2 },
        { "label": "G", "matrix": [2, 5], "x": 5.75, "y": 2 },
        { "label": "H", "matrix": [2, 6], "x": 7.75, "y": 2 },
        { "label": "J", "matrix": [2, 7], "x": 8.75, "y": 2 },
        { "label": "K", "matrix": [2, 8], "x": 9.75, "y": 2 },
        { "label": "L", "matrix": [2, 9], "x": 10.75, "y": 2 },
        { "label": ":", "matrix": [2, 10], "x": 11.75, "y": 2 },
        { "label": "\"", "matrix": [2, 11], "x": 12.75, "y": 2 },
        { "label": "Enter", "matrix": [4, 12], "x": 13.75, "y": 2 },
        { "label": "Shift", "matrix": [3, 0], "x": 1, "y": 3 },
        { "label": "Z", "matrix": [3, 1], "x": 2.25, "y": 3 },
        { "label": "X", "matrix": [3, 2], "x": 3.25, "y": 3 },
        { "label": "C", "matrix": [3, 3], "x": 4.25, "y": 3 },
        { "label": "V", "matrix": [3, 4], "x": 5.25, "y": 3 },
        { "label": "B", "matrix": [3, 5], "x": 6.25, "y": 3 },
        { "label": "N", "matrix": [3, 6], "x": 8.25, "y": 3 },
        { "label": "M", "matrix": [3, 7], "x": 9.25, "y": 3 },
        { "label": "<", "matrix": [3, 8], "x": 10.25, "y": 3 },
        { "label": ">", "matrix": [3, 9], "x": 11.25, "y": 3 },
        { "label": "?", "matrix": [3, 10], "x": 12.25, "y": 3 },
        { "label": "Shift", "matrix": [3, 11], "x": 13.25, "y": 3 },
        { "label": "Ctrl", "matrix": [4, 0], "x": 0.5, "y": 4 },
        { "label": "Win", "matrix": [4, 1], "x": 1.5, "y": 4 },
        { "label": "Alt", "matrix": [4, 2], "x": 2.5, "y": 4 },
        { "label": "Raise", "matrix": [4, 3], "x": 3.75, "y": 4 },
        { "label": "Delete", "matrix": [4, 4], "x": 5, "y": 4 },
        { "label": "Space", "matrix": [4, 5], "x": 6, "y": 4 },
        { "label": "Enter", "matrix": [4, 6], "x": 7.75, "y": 4 },
        { "label": "Backspace", "matrix": [4, 7], "x": 8.75, "y": 4 },
        { "label": "Lower", "matrix": [4, 8], "x": 10, "y": 4 },
        { "label": "Alt", "matrix": [4, 9], "x": 11.25, "y": 4 },
        { "label": "Menu", "matrix": [4, 10], "x": 12.25, "y": 4 },
        { "label": "Ctrl", "matrix": [4, 11], "x": 13.25, "y": 4 }
      ]
    }
  },

  "diode_direction": "COL2ROW",
  "matrix_pins": {
    "cols": ["F0", "F1", "F4", "F5", "F6", "F7", "D4", "D5", "D3", "D2", "D1", "D0", "B7"],
    "rows": ["B6", "B5", "B4", "D7", "D6"]
  },

  "rgblight": {
    "led_count": 8,
    "animations": {
      "alternating": false,
      "breathing": true,
      "christmas": false,
      "knight": false,
      "rainbow_mood": true,
      "rainbow_swirl": true,
      "rgb_test": false, 
      "snake": true,
      "static_gradient": true,
      "twinkle": true
    },
    "sleep": true,
    "hue_steps": 4,
    "saturation_steps": 17,
    "brightness_steps": 9
},

  "ws2812": {
    "pin": "C7"
  }

}
